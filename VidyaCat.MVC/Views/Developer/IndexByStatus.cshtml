@model IEnumerable<VidyaCat.Data.Developer>

@{
    ViewBag.Title = "IndexByStatus";
}

<h2>IndexByStatus</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.DeveloperName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Region)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IsMajor)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IsActive)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.DeveloperName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Region)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IsMajor)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IsActive)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.DeveloperID }) |
            @Html.ActionLink("Details", "Details", new { id=item.DeveloperID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.DeveloperID })
        </td>
    </tr>
}

</table>
